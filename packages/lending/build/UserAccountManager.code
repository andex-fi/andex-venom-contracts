.macro public_function_selector
DUP
PUSHINT 1756716863
LEQ
IFJMPREF {
	DUP
	PUSHINT 185736617
	EQUAL
	IFJMPREF {
		CALL $modules$
	}
	DUP
	PUSHINT 1120319739
	EQUAL
	IFJMPREF {
		CALL $existingModules$
	}
	DUP
	PUSHINT 1729765629
	EQUAL
	IFJMPREF {
		CALL $userAccountCodes$
	}
	DUP
	PUSHINT 1756716863
	EQUAL
	IFJMPREF {
		CALL $constructor$
	}
}
DUP
PUSHINT 1846468625
LEQ
IFJMPREF {
	DUP
	PUSHINT 1793525770
	EQUAL
	IFJMPREF {
		CALL $marketAddress$
	}
	DUP
	PUSHINT 1846468625
	EQUAL
	IFJMPREF {
		CALL $contractCodeVersion$
	}
}

